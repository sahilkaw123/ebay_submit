        <div class="col-md-1">
            <select class="form-control" ng-model="option" ng-options="x.model for x in options" style="width:100px"></select>
        </div>
        <div class="col-md-1">&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbspView:</div>
        <div class="col-md-1">
            <select class="form-control" ng-model="option" ng-options="x.model for x in options" style="width:50px"></select>
        </div>
    </div> <br>


    <div class="column" ng-repeat="result in myResult | orderBy:'Group_Name'"  >
        <div class="col-md-12" style="border-style:double;"  >
            <div class "container">


            <div class "row">
            <div class="col-md-3" style="border-style:ridge;"   >

                <a href="">&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp<img src={{result.pic}} width="200" height="200" ></a>
            </div>
            <div class="col-md-7 col-md-offset-1" >
                <font color="blue" size="3"><Strong><b><u>{{result.category}} - </u></b></Strong></font>
                <font color="blue" size="3"><Strong><u><b>{{result.Group_Name}}</u></Strong></font> </p>
                <p> <font color="black" size="3">{{result.ITEM_NAME}} - {{result.ITEM_DESC}}</b></Strong></font></p>
                <p> <a ng-click="showDetail(result.ITEM_CODE);" <Strong>Code: ##</Strong>{{result.ITEM_CODE}}</a>  </p>

                <p><a   ng-show="result.BID == 0"><font color="black" size="3"><Strong><b>Price $ {{result.ITEM_PRICE}} </b></Strong></font></a> </p>
                <p><a  ng-show="result.BID == 1"><font color="black" size="3"><Strong><b>Bid Price $ {{result.ITEM_PRICE}}</b></Strong></font> </a> <p>
                <p> <a  ng-click="showBid(result.ITEM_CODE);" ng-show="result.BID == 1 && result.BidStat==0"><font color="red" size="2"><b><u>[{{result.num}} bids]</u></b></font></a></p>
                <p> <a  ng-click="showBid(result.ITEM_CODE);" ng-show="result.BID == 1 && result.BidStat==1"><font color="red" size="2"><b><u>[Bid for this product has ended]</u></b></font></a></p>
                <p><b>Condition: {{result.COND}}</b></p>
                <p><font color="black" size="1"><Strong><b>Ships FAST! - </b></Strong></font> <font color="black" size="1"><Strong> Buy It Now </Strong> &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp
                        &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp

                        <a  ng-show="result.ITEM_PRICE >=60"><font color="black" size="1"><Strong><b> Free Shipping </b></Strong></a>
                        <a ng-show="result.ITEM_PRICE < 60"><font color="black" size="1"><Strong><b> Shipping cost $ 7.59 </b></Strong>

